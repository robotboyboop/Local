					local func = rawget(v, "Update");
					if toggle then
						hookfunction(func, newlclosure(function(self)
							rawset(self, "_spray", {0, 0, 0});
						end));
					else
						restorefunction(func)
					end;
				end;			
			end;
		end;
	end,

});
